<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" styleClass="game-over-window">
    <Button fx:id="restartButton" alignment="CENTER" layoutX="200.0" layoutY="525.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="350.0" text="Restart">
        <font>
            <Font name="Verdana" size="36.0"/>
        </font>
    </Button>
    <Button fx:id="menuButton" alignment="CENTER" layoutX="730.0" layoutY="525.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="350.0" text="Main Menu">
        <font>
            <Font name="Verdana" size="36.0"/>
        </font>
    </Button>
    <Label fx:id="titleLabel" alignment="CENTER" layoutX="140.0" layoutY="25.0" prefHeight="200.0" prefWidth="1000.0" text="Game Over">
        <font>
            <Font name="Verdana Bold" size="127.0"/>
        </font>
    </Label>
</Pane>
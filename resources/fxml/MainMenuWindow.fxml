<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" styleClass="main-menu-window">
    <Button fx:id="startButton" alignment="CENTER" layoutX="200.0" layoutY="525.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="350.0" text="New Game">
        <font>
            <Font name="Verdana" size="36.0"/>
        </font>
    </Button>
    <Button fx:id="quitButton" alignment="CENTER" layoutX="730.0" layoutY="525.0" mnemonicParsing="false" prefHeight="100.0" prefWidth="350.0" text="Quit">
        <font>
            <Font name="Verdana" size="36.0"/>
        </font>
    </Button>
    <Label alignment="CENTER" layoutX="140.0" layoutY="25.0" prefHeight="200.0" prefWidth="1000.0" text="Mineslayer">
        <font>
            <Font name="Verdana Bold" size="127.0"/>
        </font>
    </Label>
    <Label id="credits" layoutX="10.0" layoutY="675.0" prefHeight="35.0" prefWidth="1280.0" text="Created by Jacob Pitzer, Justin Penkalski, and Peter Humphreys. Some art created by Kenney (kenney.nl).">
        <font>
            <Font name="Verdana" size="18.0"/>
        </font>
    </Label>
</Pane>